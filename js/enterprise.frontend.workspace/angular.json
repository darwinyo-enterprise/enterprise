{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "commerce.admin-app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/commerce.admin-app",
            "index": "apps/commerce.admin-app/src/index.html",
            "main": "apps/commerce.admin-app/src/main.ts",
            "tsConfig": "apps/commerce.admin-app/src/tsconfig.app.json",
            "polyfills": "apps/commerce.admin-app/src/polyfills.ts",
            "assets": [
              { "glob": "**/*", "input": "apps/commerce.admin-app/src/assets/", "output": "/assets/" },
              { "glob": "favicon.ico", "input": "/apps/commerce.admin-app/src", "output": "/" }
            ],
            "styles": [
              "../../sass/enterprise.globalstyle/src/_main.scss",
              "apps/commerce.admin-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "apps/commerce.admin-app/src/environments/environment.ts",
                  "with": "apps/commerce.admin-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "commerce.admin-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "commerce.admin-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "commerce.admin-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/commerce.admin-app/src/../../../test.js",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "apps/commerce.admin-app/src/polyfills.ts",
            "tsConfig": "apps/commerce.admin-app/src/../../../tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "../../sass/enterprise.globalstyle/src/_main.scss",
              "apps/commerce.admin-app/src/styles.scss"
            ],
            "assets": [
              { "glob": "**/*", "input": "apps/commerce.admin-app/src/assets/", "output": "/assets/" },
              { "glob": "favicon.ico", "input": "/apps/commerce.admin-app/src", "output": "/" }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "./tsconfig.spec.json",
              "apps/commerce.admin-app/src/tsconfig.app.json",
              "apps/commerce.client-app/src/tsconfig.app.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/server/commerce.admin-app",
            "main": "main.server.ts",
            "tsConfig": "tsconfig.server.json"
          }
        }
      }
    },
    "commerce.admin-app-e2e": {
      "root": "./apps/commerce.admin-app/e2e",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./apps/commerce.admin-app/e2e/protractor.conf.js",
