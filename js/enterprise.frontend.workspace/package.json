  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "doc:build": "compodoc -p tsconfig.json --theme material",
    "doc:serve": "compodoc -s --port 9999",
    "doc:buildandserve": "compodoc -p tsconfig.json --theme material -s --port 9999",
