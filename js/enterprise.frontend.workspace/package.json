  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "doc:build": "compodoc -p tsconfig.json --theme material",
    "doc:serve": "compodoc -s --port 9999",
    "doc:buildandserve": "compodoc -p tsconfig.json --theme material -s --port 9999",
    "affected:apps": "./node_modules/.bin/nx affected apps",
    "affected:build": "./node_modules/.bin/nx affected build",
    "affected:e2e": "./node_modules/.bin/nx affected e2e",
    "format": "./node_modules/.bin/nx format write",
    "format:write": "./node_modules/.bin/nx format write",
    "format:check": "./node_modules/.bin/nx format check",
    "build:universal": "npm run build:client-and-server-bundles:dev && npm run webpack:server",
    "serve:universal": "node ./apps/commerce.admin-app/dist/server.js",
    "build:client-and-server-bundles:prod": "ng build --prod --app commerce.admin-app && ng build --prod --app commerce.admin-app.server --output-hashing=false",
    "build:client-and-server-bundles:dev": "ng build --app commerce.admin-app && ng build --app commerce.admin-app.server --output-hashing=false",
    "webpack:server": "webpack --config ./apps/commerce.admin-app/webpack.server.config.js --progress --colors",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "build": "webpack",
    "start": "webpack-dev-server --port=4200",
    "test": "karma start ./karma.conf.js",
    "e2e": "protractor ./protractor.conf.js"
  },
